#include <iostream>

using namespace std;


int main(){
    int n,sum1=0,sum2=0;
    int A[n][n];
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            cin>>A[i][j];
        }
    }
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            sum1+=A[i][i];
        }
    }
    
    
    
}
