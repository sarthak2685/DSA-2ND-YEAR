#include <stdio.h>

void update(int *a,int *b) {
    int c,k;
    c=*a+*b;
    k=*a-*b;
    *a=c;
    *b=abs(k);
    
    // Complete this function    
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);

    return 0;
}
