#include <bits/stdc++.h>

using namespace std;

int main(){
    int n;
    cin>>n;
    int A[n];
    for(int i=0;i<n;i++){
        cin>>A[i];
    }
    for(int i=0;i<n;i++){
        if(A[i]<38){
            cout<<A[i]<<endl;
        }
        else if(A[i]>=38){
            int a,b,c,d;
            a=A[i]/5;
            b=a+1;
            c=b*5;
            d=c-A[i];
            if(d<3){
                cout<<c<<endl;
            }
            else {
            cout<<A[i]<<endl;
            }
            
            
        }
    }
    return 0;
}
